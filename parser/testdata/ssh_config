Host *
    UseKeyChain yes

Host a.bitbucket.org
    IdentityFile ~/.ssh/id-rsa
    HostName bitbucket.org
    User my@mail.org
    UseKeyChain yes
    AddKeysToAgent yes

Host b.bitbucket.org
    IdentityFile ~/.ssh/id-rsa
    HostName bitbucket.org
    UseKeyChain yes
    AddKeysToAgent yes

Host development
    IdentityFile ~/.ssh/id-rsa
    User my-user
    HostName 1.2.3.4
    Port 22
    AddKeysToAgent yes

Host staging
    IdentityFile ~/.ssh/id-rsa
    HostName 1.2.3.4
    User my-user
    LocalForward 127.0.0.1:27012 127.0.0.1:27012
    Port 22
Host production
    IdentityFile /home/user/.ssh/id-rsa.pem
    HostName host.compute.amazonaws.com
    LocalForward 3306 host.rds.amazonaws.com:3306
    User my-yser
    Port 22
Host staging-web
    IdentityFile /home/user/.ssh/id-rsa.pem
    HostName 1.2.3.4
    User ec2-User
    Port 22
Host medusa-web
    HostName 1.2.3.4
    User calvernaz
    Port 22
Host staging-web
    IdentityFile /home/user/.ssh/id-rsa.pem
    HostName 1.2.3.4
    User my-user
    Port 22
Host registry
    IdentityFile /home/user/.ssh/id-rsa.pem
    HostName 1.2.3.4
    User my-user
    Port 22
